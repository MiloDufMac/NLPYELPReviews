        "id": "nqetFkTYjAZD"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Bag-of-Words\n",
        "vectorizer = CountVectorizer(max_features=1000)\n",
        "X = vectorizer.fit_transform(df['Processed Text'])\n",
        "y = df['Sentiment_Label']"
      ],
      "metadata": {
        "id": "z8lNrBC9i_kd"
      },
      "execution_count": null,
      "outputs": []
    },
