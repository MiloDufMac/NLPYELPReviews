      "cell_type": "code",
      "source": [
        "def label_encoding(stars):\n",
        "    if stars in [1, 2]:\n",
        "        return 'Negative'\n",
        "    elif stars == 3:\n",
        "        return 'Neutral'\n",
        "    elif stars in [4, 5]:\n",
        "        return 'Positive'\n",
        "\n",
        "# Apply the function to your DataFrame\n",
        "df['Sentiment'] = df['Stars'].apply(label_encoding)"
      ],
      "metadata": {
        "id": "JupJeYg-16t2"
      },
      "execution_count": null,
      "outputs": []
    },
    {
